{
  "name": "@tankswap/universal-router-sdk",
  "version": "1.4.14",
  "description": "ðŸ›  An SDK for building applications on top of Pancakeswap.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tankswap/tank-frontend.git",
    "directory": "packages/universal-router-sdk"
  },
  "keywords": [
    "tankswap"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "coverage": "vitest run --coverage",
    "test": "vitest --run",
    "test:update": "vitest -u",
    "test:label": "vitest -t label --run",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "format:check": "prettier --check --loglevel error \"{src,test}/**/*.{js,jsx,ts,tsx,json}\"",
    "format:write": "prettier --write \"{src,test}/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint '{src,test}/**/*.{js,jsx,ts,json}'"
  },
  "devDependencies": {
    "@tankswap/stable-swap-sdk": "workspace:*",
    "@tankswap/tokens": "workspace:*",
    "@types/node": "^15.12.2",
    "tslib": "^2.3.0",
    "tsup": "^6.7.0",
    "@types/lodash": "*"
  },
  "dependencies": {
    "@tankswap/chains": "workspace:*",
    "@tankswap/permit2-sdk": "workspace:*",
    "@tankswap/sdk": "workspace:*",
    "@tankswap/smart-router": "workspace:*",
    "@tankswap/v3-sdk": "workspace:*",
    "@tankswap/infinity-sdk": "workspace:*"
  },
  "peerDependencies": {
    "viem": "catalog:",
    "tiny-invariant": "*",
    "abitype": "*",
    "lodash": "*"
  }
}
